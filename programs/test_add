$PROGRAM
    "TEST"
$DATA
    "0123" ; Will be parsed as "0123"
    0123   ; Will be parsed as 83
    0x123  ; Will be parsed as 291
    123h   ; Will be parsed as 291
    123    ; Will be parsed as 123
    "TEST"
$CODE
    PUSH 14
    PUSH 65535
    PUSH 1
    PUSH 1
    DEC
    POPM
    HALT
$END
